<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1><br>
    </section>
    <%= link_to request.base_url + "/" + @user.webPage,request.base_url + "/" + @user.webPage %>
  </aside>
     <div class="col-md-8">
      <h3>Proyects</h3>
      <% if @user.feed.any? %>
  <ol class="proyects">
    <%= render  @user.feed %>
  </ol>
<% end %>
</div>
</div>